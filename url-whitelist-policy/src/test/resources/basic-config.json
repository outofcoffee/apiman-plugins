{
  "removePathPrefix": true,
  "whitelist": [
    {
      "regex": "/allow/.*",
      "methodGet": true,
      "methodPost": true,
      "methodPut": true,
      "methodPatch": true,
      "methodDelete": true
    },
    {
      "regex": "/deny/.*",
      "methodGet": false,
      "methodPost": false,
      "methodPut": false,
      "methodPatch": false,
      "methodDelete": false
    }
  ]
}
